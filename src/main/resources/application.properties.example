spring.application.name=video-creator

spring.datasource.url=jdbc:postgresql://localhost:5435/dbname
spring.datasource.username=username
spring.datasource.password=password
spring.datasource.driver-class-name=org.postgresql.Driver

spring.jpa.database-platform=org.hibernate.dialect.PostgreSQLDialect
spring.jpa.hibernate.ddl-auto=none
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true

# Replace with random string or placeholder
myapp.jwt.secret=YOUR_JWT_SECRET_KEY

# Replace with placeholders
cloudinary.cloud_name=YOUR_CLOUD_NAME
cloudinary.api_key=YOUR_API_KEY
cloudinary.api_secret=YOUR_API_SECRET

# API Keys - replace with placeholders
myapp.parameters.eleven-labs-key=YOUR_ELEVEN_LABS_KEY

myapp.parameters.groq-url=https://api.groq.com/openai/v1/audio/speech
myapp.parameters.groq-secret-key=YOUR_GROQ_SECRET_KEY

# Other API configurations with placeholders
myapp.parameters.camb-url=https://client.camb.ai/apis
myapp.parameters.camb-secret-key=YOUR_CAMB_SECRET_KEY

# Additional configuration placeholders
myapp.parameters.togetherAI-url=https://api.together.xyz/v1/images/generations
myapp.parameters.togetherAI-secret-key=YOUR_TOGETHER_AI_KEY

myapp.parameters.clipdrop-url=https://clipdrop-api.co/text-to-image/v1
myapp.parameters.clipdrop-secret-key=YOUR_CLIPDROP_KEY

myapp.parameters.shot-stack-url=https://api.shotstack.io/edit/stage/render
myapp.parameters.shot-stack-secret-key=YOUR_SHOTSTACK_KEY

myapp.parameters.gemini-url=https://generativelanguage.googleapis.com/v1beta/models
myapp.parameters.gemini-secret-key=YOUR_GEMINI_KEY

myapp.parameters.open-router-url=https://openrouter.ai/api/v1/chat/completions
myapp.parameters.open-router-secret-key=YOUR_OPENROUTER_KEY

myapp.parameters.google-client-id=YOUR_GOOGLE_CLIENT_ID
myapp.parameters.google-client-secret=YOUR_GOOGLE_CLIENT_SECRET

myapp.parameters.google-token-issue=YOUR_TOKEN_ISSUE
myapp.parameters.google-auth-url=YOUR_AUTH_URL
myapp.parameters.google-callback-url=YOUR_CALLBACK_URL
myapp.parameters.youtube-stats-url=YOUR_STATS_URL
myapp.parameters.google-api-key=YOUR_GOOGLE_API_KEY

myapp.parameters.tiktok-client-key=YOUR_TIKTOK_CLIENT_KEY
myapp.parameters.tiktok-client-secret=YOUR_TIKTOK_CLIENT_SECRET
myapp.parameters.tiktok-api-url=https://open.tiktokapis.com/v2

myapp.parameters.front-end.base-url=http://localhost:5173

spring.servlet.multipart.max-file-size=512MB
spring.servlet.multipart.max-request-size=512MB